{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "id": "f6da93f0-c94f-11e9-9e80-0d4d4963d6f0",
  "version": "0.0.1",
  "packageName": "com.swoopanalytics.swoopteams",
  "developer": {
    "name": "SWOOP Analytics Pty Ltd",
    "websiteUrl": "https://{{HOSTNAME}}",
    "privacyUrl": "https://{{HOSTNAME}}/privacy.html",
    "termsOfUseUrl": "https://{{HOSTNAME}}/tou.html"
  },
  "name": {
    "short": "Swoop Analytics",
    "full": "Measure and improve engagement and collaboration"
  },
  "description": {
    "short": "Collaboration Analytics",
    "full": "Extensive dashboard for analysing your social collaborations. Analyse collaboration patterns across the enterprise and within business units."
  },
  "icons": {
    "outline": "icon-outline.png",
    "color": "icon-color.png"
  },
  "accentColor": "#FFFFFF",
  "configurableTabs": [
    {
      "configurationUrl": "https://{{HOSTNAME}}/swoopAnalyticsTab/config.html",
      "canUpdateConfiguration": true,
      "scopes": [
        "team"
      ],
      "sharePointPreviewImage": "https://{{HOSTNAME}}/assets/swoopAnalyticsTab-preview.png",
      "supportedSharePointHosts": [
        "sharePointFullPage",
        "sharePointWebPart"
      ]
    }
  ],
  "staticTabs": [],
  "bots": [],
  "connectors": [],
  "composeExtensions": [],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "{{HOSTNAME}}"
  ]
}
